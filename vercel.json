{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/posts/(.*)",
            "dest": "routes/posts.js"
        },
        {
            "src": "/api/users/(.*)",
            "dest": "routes/users.js"
        }
    ]
}