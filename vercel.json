{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 60,
      "memory": 3008,
      "bodySize": "50mb"
    }
  }
}